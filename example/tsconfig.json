{
  "compilerOptions": {
    "strictNullChecks": true,
    "moduleResolution": "node",
    "jsx": "react",
    "noUnusedParameters": true,
    "noUnusedLocals": true,
    "allowSyntheticDefaultImports": true,
    "target": "es6",
    "skipLibCheck": true,
    "plugins": [
      {
        "name": "typescript-tslint-plugin"
      }
    ]
  }
}

